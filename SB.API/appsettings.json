{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "ConnectionStrings": {
        "DefaultConnection": "Server=localhost;Database=TestDB;Trusted_Connection=True;TrustServerCertificate=True;"
    },
    "Jwt": {
        "Key": "SecretKey12345",
        "Issuer": "backendapi",
        "Audience": "frontendapp"
    },
    "Serilog": {
        "MinimumLevel": "Information",
        "WriteTo": [
            {
                "Name": "File",
                "Args": { "path": "Logs/logs.txt" }
            }
        ]
    },
    "AllowedHosts": "*"
}
